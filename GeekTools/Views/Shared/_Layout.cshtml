@using System.Web.Optimization;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

    <meta name="SKYPE_TOOLBAR" content="SKYPE_TOOLBAR_PARSER_COMPATIBLE">



    <title>GeekTools</title>
    <link rel="shortcut icon" type="image/gif" href="~/Images/favicon.gif">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=2">


    <link rel="canonical" href="#">
    <meta property="og:locale" content="en_US">
    <meta property="og:type" content="article">
    <meta property="og:title" content="Home | GeekTools">
    <meta property="og:url" content="#">
    <meta property="og:site_name" content="GeekTools">
    <meta name="twitter:card" content="summary">
    <meta name="twitter:title" content="Homw | GeekTools">
    <meta name="twitter:domain" content="GeekTools">
    <!-- / Yoast SEO plugin. -->

    <link rel="alternate" type="application/rss+xml" title="GeekTools » About Us Comments Feed" href="#">
    <link rel="EditURI" type="application/rsd+xml" title="RSD" href="#">
    <link rel="wlwmanifest" type="application/wlwmanifest+xml" href="#">
</head>
<body class="animate">
    <header class="mast-head full">
        <div class="sleeve">
            <div class="logo">
                <a href="#">
                    <img src="~/Images/logo.png" alt="GeekTools" />
                    <img src="/Images/logo2.png" style="margin-left: -35px; padding-bottom: 45px;">
                </a>
               
            </div>

            <a href="#" class="nav-toggle">Menu</a>
        </div>
    </header>

    <div class="navigation">
        <a href="#" class="nav-toggle nav-toggle-close">Menu <span>Close</span></a>
        <nav class="main-nav">
            <div class="main-nav-sleeve">
                <ul id="menu-main-menu" class="">
                    <li class="nav-item current"><a href="www.google.com"><span>Home</span></a></li>
                    <li class="nav-item"><a href="#"><span>What is GeekTools?</span></a></li>
                    <li class="nav-item"><a href="#"><span>Journey to GeekTools</span></a></li>
                    <li class="nav-item"><a href="#"><span>The Articles</span></a></li>
                    <li class="nav-item "><a href="#"><span>About Us</span></a></li>
                    <li class="nav-item"><a href="#"><span>Contact</span></a></li>
                </ul>
            </div>
        </nav>
        <nav class="sub-nav">
            <div class="sub-nav-sleeve">
                <ul id="menu-main-sub-menu" class="">
                    <li class="nav-item"><a href="#"><span>Terms &amp; Conditions</span></a></li>
                    <li class="nav-item"><a href="#"><span>Privacy</span></a></li>
                </ul>
            </div>
        </nav>
    </div>
    <main class="main full">
     

    @RenderBody()

    

    <footer class="main-footer full mod">
        <div class="sleeve">
            <div class="footer-logo">
                <a href="#">
                    <img src="~/Images/logo.png" alt="GeekTools"></a>
                <p>
                    GeekTools is a strategic consulting firm.
					We help organisations articulate who
					they are and why they matter.
			
                </p>
                <small>©2015 GeekTools Limited.</small>
            </div>
            <nav class="nav">
                <div class="column">
                    <p class="nav-title">Company</p>
                    <div class="the-nav">
                        <ul id="menu-company" class="">
                            <li class="nav-item current"><a href="#"><span>Home</span></a></li>
                            <li class="nav-item"><a href="#"><span>About</span></a></li>
                            <li class="nav-item"><a href="#"><span>Contact</span></a></li>
                        </ul>
                    </div>
                </div>
                <div class="column">
                    <p class="nav-title">Discover</p>
                    <div class="the-nav">
                        <ul id="menu-discover" class="">
                            <li class="nav-item">
                                <a href="#"><span>What is GeekTools?</span></a></li>
                            <li class="nav-item"><a href="#"><span>Your Journey to GeekTools</span></a></li>
                            <li class="nav-item">
                                <a href="#"><span>The Articles</span></a>

                            </li>

                        </ul>

                    </div>

                </div>
                               <div class="column social">
                    <p class="nav-title">Social</p>
                    <ul>
                        <li class="twitter">
                            <span>
                                <img src="~/Images/social-twitter-dark.svg" alt="" />

                            </span>
                            <a href="#" target="_blank">Twitter</a>
                        </li>
                        <li class="facebook">
                            <span>
                                <img src="~/Images/social-facebook-dark.svg" alt="" />

                            </span>
                            <a href="#" target="_blank">Facebook</a>
                        </li>
                        <li class="linkedin">
                            <span>
                                <img src="~/Images/social-linkedin-dark.svg" alt="" />

                            </span>
                            <a href="#" target="_blank">LinkedIn</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
    </footer>


         </main>
    @Scripts.Render("~/bundles/Jquery")
    @Scripts.Render("~/bundles/Site")

    @* Render the custom bundles *@
    @Styles.Render("~/bundles/Css")

</body>
</html>
